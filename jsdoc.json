{
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "src/core/util"
        ],
        "includePattern": ".js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/jsdoc"
    },
    "sourceType": "module"
}